{
  "name": "redux-balloon",
  "version": "1.0.0",
  "description": "Lightweight business framework for Redux apps",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/IAMSUPERMONKEY/redux-balloon.git"
  },
  "bugs": "https://github.com/IAMSUPERMONKEY/redux-balloon/issues",
  "keywords": [
    "redux",
    "reducer",
    "state",
    "immutable",
    "flux",
    "redux-actions",
    "reselect",
    "redux-saga",
    "react",
    "react-redux",
    "wepy",
    "framework",
    "elm",
    "model"
  ],
  "authors": [
    "Xie Kai <xiekai0601@gmail.com> (https://github.com/deepfunc)"
  ],
  "main": "lib/index.js",
  "unpkg": "dist/redux-balloon.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "es",
    "lib",
    "wepy",
    "src"
  ],
  "scripts": {
    "build": "run-s clean && run-p build:*",
    "build:esm": "cross-env BABEL_ENV=esm babel src --out-dir es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:wepy": "gulp buildWePY",
    "build:umd": "cross-env BABEL_ENV=umd rollup -c && es-check es5 dist/redux-balloon.min.js",
    "clean": "rimraf dist es lib wepy",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm test -- --coverage",
    "coveralls": "npm run test:cov && cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "@types/redux-actions": "^2.6.1",
    "invariant": "^2.2.4",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "seamless-immutable": "^7.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "coveralls": "^3.0.5",
    "cross-env": "^5.2.0",
    "es-check": "^5.0.0",
    "eslint": "^5.10.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-replace": "^1.0.0",
    "gulp-shell": "^0.7.0",
    "jest": "^24.8.0",
    "npm-run-all": "^4.1.5",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.4",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^3.0.0",
    "run-sequence": "^2.2.1",
    "sinon": "^7.2.0",
    "typescript": "^3.7.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
